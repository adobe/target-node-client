{
  "name": "@adobe/target-tools",
  "version": "1.6.0",
  "description": "",
  "main": "dist/index.js",
  "browser": "dist/index.browser.js",
  "//module": "src/index.ts",
  "files": [
    ".eslintrc.js",
    "babel.config.js",
    "jest.config.js",
    "delivery-api-client/**/*",
    "dist/*",
    "src/**/!(*.spec).js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "format": "prettier '**/*.ts' '**/*.js' '**/*.json' --write --config '../../.prettierrc.json' --ignore-path '../../.prettierignore'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "NODE_ENV=production rollup -c",
    "pretest": "npm run format && npm run lint",
    "test": "NODE_ENV=development jest",
    "prepublishOnly": "echo //${NPM_REGISTRY}/:_authToken=${NPM_AUTH_TOKEN} > .npmrc",
    "prepare": "rollup --config"
  },
  "dependencies": {
    "@adobe/reactor-object-assign": "^1.0.0",
    "parse-uri": "^1.0.0",
    "performance-now": "^2.1.0"
  },
  "devDependencies": {
    "core-js": "^3.6.5",
    "http-status-codes": "^1.4.0"
  },
  "author": "",
  "license": "Apache-2.0"
}
