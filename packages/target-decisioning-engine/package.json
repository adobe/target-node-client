{
  "name": "@adobe/target-decisioning-engine",
  "version": "1.8.0",
  "description": "Adobe Target JavaScript decisioning engine",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/*",
    "types/*",
    "src/**/!(*.spec).js",
    "test/test.utils.js",
    "test/schema/**/*.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "//lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "lint": "",
    "format": "prettier '**/*.ts' '**/*.js' '**/*.json' --write --config '../../.prettierrc.json' --ignore-path '../../.prettierignore'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "NODE_ENV=production rollup -c",
    "pretest": "npm run format && npm run lint",
    "test": "NODE_ENV=development jest",
    "prepublishOnly": "echo //${NPM_REGISTRY}/:_authToken=${NPM_AUTH_TOKEN} > .npmrc",
    "prepare": "rollup --config"
  },
  "dependencies": {
    "@adobe/target-tools": "^1.6.0",
    "json-logic-js": "^2.0.0"
  },
  "devDependencies": {
    "fast-text-encoding": "^1.0.3",
    "http-status-codes": "^1.4.0",
    "https-proxy-agent": "^5.0.0",
    "mockdate": "^2.0.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1"
  },
  "author": "",
  "license": "Apache-2.0"
}
